<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent" android:layout_height="match_parent"
        tools:context=".MainActivity" android:id="@+id/linearLayoutCompat">

    <!--ヘッダー-->
    <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" tools:layout_editor_absoluteY="79dp"
            tools:layout_editor_absoluteX="0dp">

        <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent" app:srcCompat="@drawable/header" android:id="@+id/imageView4"
                android:contentDescription="@+string/anyStringValue"

                android:layout_marginBottom="8dp"
                android:scaleType="fitStart"/>

        <androidx.viewpager.widget.ViewPager
                android:id="@+id/pager"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="165dp">

            <com.google.android.material.tabs.TabLayout
                    android:id="@+id/tab_layout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:tabIndicatorColor="@color/ハイライト"
                    app:tabTextColor="@color/colorContainer"
                    app:tabSelectedTextColor="@color/ハイライト"
                    app:tabIndicatorGravity="bottom" app:tabMode="scrollable" app:tabGravity="fill"
                    app:tabIconTintMode="src_over"
                    app:tabTextAppearance="@style/TextAppearance.AppCompat" app:tabPaddingStart="40dp"
                    app:tabPaddingEnd="40dp">
                <!--表紙-->
                <com.google.android.material.tabs.TabItem

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                />
                <!--プロフィール画面-->
                <com.google.android.material.tabs.TabItem

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                />
                <!--もちもの-->
                <com.google.android.material.tabs.TabItem

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                />
                <!--　21日-->
                <com.google.android.material.tabs.TabItem

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                />
                <!--22日-->
                <com.google.android.material.tabs.TabItem

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                />
                <!--23日-->
                <com.google.android.material.tabs.TabItem

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                />
                <!--お店-->
                <com.google.android.material.tabs.TabItem

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                />
                <!--ホテル-->
                <com.google.android.material.tabs.TabItem

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                />
                <!--日記-->
                <com.google.android.material.tabs.TabItem

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                />


            </com.google.android.material.tabs.TabLayout>

        </androidx.viewpager.widget.ViewPager>
    </FrameLayout>




</androidx.constraintlayout.widget.ConstraintLayout>

